<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#e8e8e8">

        <TextView
            android:id="@+id/tvLabelTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#000000"
            android:gravity="center_horizontal"
            android:text="个人中心"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/dp_28"
            />

        <ImageView
            android:id="@+id/ivCs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/tvLabelTitle"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/dp_20"
            android:src="@drawable/ic_icon_cs"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clTopLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/tvLabelTitle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_37"
            >

            <TextView
                android:id="@+id/tvAccountNum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:textSize="14sp"
                android:textColor="#000000"
                android:text="dada123456789012"
                />

            <TextView
                android:id="@+id/tvNickName"
                android:layout_width="@dimen/dp_111"
                android:layout_height="@dimen/dp_30"
                android:background="@drawable/bg_white_15_corner"
                app:layout_constraintTop_toBottomOf="@+id/tvAccountNum"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="@dimen/dp_7"
                android:textSize="@dimen/sp_12"
                android:gravity="center"
                android:text="昵称2-15个字符"
                />

            <TextView
                android:layout_width="@dimen/dp_67"
                android:layout_height="@dimen/dp_19"
                app:layout_constraintTop_toTopOf="@+id/clPlayType"
                app:layout_constraintBottom_toBottomOf="@+id/clPlayType"
                app:layout_constraintStart_toStartOf="@+id/clPlayType"
                android:layout_marginStart="@dimen/dp_13"
                android:textSize="@dimen/sp_12"
                android:text="高级玩家"
                android:gravity="center|end"
                android:paddingStart="@dimen/dp_6"
                android:paddingEnd="@dimen/dp_6"
                android:background="@drawable/bg_white_10_corner"
                />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clPlayType"
                android:layout_width="@dimen/dp_25"
                android:layout_height="@dimen/dp_25"
                app:layout_constraintTop_toTopOf="@+id/tvNickName"
                app:layout_constraintStart_toEndOf="@+id/tvNickName"
                app:layout_constraintBottom_toBottomOf="@+id/tvNickName"
                android:layout_marginStart="@dimen/dp_10"
                android:background="@drawable/bg_circle_white_and_gary_board"
                >

                <ImageView
                    android:id="@+id/ivPlayType"
                    android:layout_width="@dimen/dp_11"
                    android:layout_height="@dimen/dp_11"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/tvAccountGroup"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/tvNickName"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="@dimen/dp_9"
                android:textSize="14sp"
                android:textColor="#000000"
                android:text="奖金组：1980"
                />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clUserLayout"
                android:layout_width="@dimen/dp_68"
                android:layout_height="@dimen/dp_68"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@drawable/bg_circle_white_board"
                >

                <ImageView
                    android:id="@+id/ivUser"
                    android:layout_width="@dimen/dp_30"
                    android:layout_height="@dimen/dp_30"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:src="@drawable/ic_icon_user"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clAssetsLayout"
            android:layout_width="@dimen/dp_335"
            android:layout_height="@dimen/dp_93"
            app:layout_constraintTop_toBottomOf="@+id/clTopLayout"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_20"
            android:background="@drawable/bg_white_10_corner"
            >

            <View
                android:id="@+id/viewPoint1"
                android:layout_width="@dimen/dp_11"
                android:layout_height="@dimen/dp_11"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="@dimen/dp_17"
                android:layout_marginStart="@dimen/dp_22"
                android:background="@drawable/bg_circle_light_gary_board"
                />

            <TextView
                android:id="@+id/tvLabelTotal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#434343"
                app:layout_constraintTop_toTopOf="@+id/viewPoint1"
                app:layout_constraintBottom_toBottomOf="@+id/viewPoint1"
                app:layout_constraintStart_toEndOf="@+id/viewPoint1"
                android:layout_marginStart="@dimen/dp_8"
                android:textSize="@dimen/sp_14"
                android:text="总资产"
                />

            <TextView
                android:id="@+id/tvTotalAssets"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                app:layout_constraintTop_toTopOf="@+id/tvLabelTotal"
                app:layout_constraintBottom_toBottomOf="@+id/tvLabelTotal"
                app:layout_constraintStart_toEndOf="@+id/tvLabelTotal"
                android:layout_marginStart="@dimen/dp_43"
                android:textSize="@dimen/sp_14"
                android:text="$999,999,999.000"
                />

            <ImageView
                android:id="@+id/ivShowAssets"
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_20"
                android:src="@drawable/ic_icon_eyes_form_open"
                app:layout_constraintTop_toTopOf="@+id/viewPoint1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="@+id/viewPoint1"
                android:layout_marginEnd="@dimen/dp_24"
                />

            <View
                android:id="@+id/viewPoint2"
                android:layout_width="@dimen/dp_11"
                android:layout_height="@dimen/dp_11"
                app:layout_constraintTop_toBottomOf="@+id/viewPoint1"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="@dimen/dp_14"
                android:layout_marginStart="@dimen/dp_22"
                android:background="@drawable/bg_circle_dark_gary_board"
                />

            <TextView
                android:id="@+id/tvLabelBalance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#434343"
                app:layout_constraintTop_toTopOf="@+id/viewPoint2"
                app:layout_constraintBottom_toBottomOf="@+id/viewPoint2"
                app:layout_constraintStart_toEndOf="@+id/viewPoint2"
                android:layout_marginStart="@dimen/dp_8"
                android:textSize="@dimen/sp_14"
                android:text="可提金額"
                />

            <TextView
                android:id="@+id/tvBalance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                app:layout_constraintTop_toTopOf="@+id/tvLabelBalance"
                app:layout_constraintBottom_toBottomOf="@+id/tvLabelBalance"
                app:layout_constraintStart_toEndOf="@+id/tvLabelBalance"
                android:layout_marginStart="@dimen/dp_29"
                android:textSize="@dimen/sp_14"
                android:text="$15,000"
                />

            <ProgressBar
                android:id="@+id/pbAssets"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_11"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="@dimen/dp_22"
                android:layout_marginEnd="@dimen/dp_22"
                android:layout_marginBottom="@dimen/dp_12"
                style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clChartLayout"
            android:layout_width="@dimen/dp_335"
            android:layout_height="@dimen/dp_209"
            app:layout_constraintTop_toBottomOf="@+id/clAssetsLayout"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_20"
            android:background="@drawable/bg_white_10_corner"
            >

            <TextView
                android:id="@+id/tvLabelToday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textColor="#000000"
                android:text="今日输赢"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="@dimen/dp_15"
                android:layout_marginStart="@dimen/dp_20"
                />

            <TextView
                android:id="@+id/tvTodayLostWin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textColor="#ff0000"
                android:text="-300.000"
                app:layout_constraintTop_toTopOf="@+id/tvLabelToday"
                app:layout_constraintStart_toEndOf="@+id/tvLabelToday"
                app:layout_constraintBottom_toBottomOf="@+id/tvLabelToday"
                android:layout_marginStart="@dimen/dp_10"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clPasswordLayout"
            android:layout_width="@dimen/dp_335"
            android:layout_height="@dimen/dp_48"
            app:layout_constraintTop_toBottomOf="@+id/clChartLayout"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_20"
            android:background="@drawable/bg_white_10_corner"
            >

            <ImageView
                android:id="@+id/ivPassword"
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_20"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="@dimen/dp_20"
                android:src="@drawable/ic_icon_password"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_14"
                android:textColor="#000000"
                android:text="密码管理"
                app:layout_constraintTop_toTopOf="@+id/ivPassword"
                app:layout_constraintBottom_toBottomOf="@+id/ivPassword"
                app:layout_constraintStart_toEndOf="@+id/ivPassword"
                android:layout_marginStart="@dimen/dp_10"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clEzLoginLayout"
            android:layout_width="@dimen/dp_335"
            android:layout_height="@dimen/dp_48"
            app:layout_constraintTop_toBottomOf="@+id/clPasswordLayout"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_20"
            android:background="@drawable/bg_white_10_corner"
            >

            <ImageView
                android:id="@+id/ivFinger"
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_20"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="@dimen/dp_20"
                android:src="@drawable/ic_icon_finger"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_14"
                android:textColor="#000000"
                android:text="简易登入启用"
                app:layout_constraintTop_toTopOf="@+id/ivFinger"
                app:layout_constraintBottom_toBottomOf="@+id/ivFinger"
                app:layout_constraintStart_toEndOf="@+id/ivFinger"
                android:layout_marginStart="@dimen/dp_10"
                />

            <Switch
                android:id="@+id/scEnableEz"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginEnd="@dimen/dp_16"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clSettingLayout"
            android:layout_width="@dimen/dp_335"
            android:layout_height="@dimen/dp_198"
            app:layout_constraintTop_toBottomOf="@+id/clEzLoginLayout"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_20"
            android:background="@drawable/bg_white_10_corner"
            >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clMail"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_46"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >

                <ImageView
                    android:id="@+id/ivMail"
                    android:layout_width="@dimen/dp_20"
                    android:layout_height="@dimen/dp_20"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginStart="@dimen/dp_20"
                    android:src="@drawable/ic_icon_mail"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:textColor="#000000"
                    android:text="邮箱"
                    app:layout_constraintTop_toTopOf="@+id/ivMail"
                    app:layout_constraintStart_toEndOf="@+id/ivMail"
                    app:layout_constraintBottom_toBottomOf="@+id/ivMail"
                    android:layout_marginStart="@dimen/dp_10"
                    />

                <ImageView
                    android:layout_width="@dimen/dp_16"
                    android:layout_height="@dimen/dp_16"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginEnd="@dimen/dp_19"
                    android:src="@drawable/ic_icon_next"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/viewLine1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                app:layout_constraintTop_toBottomOf="@+id/clMail"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:background="#d6d6d6"
                />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clMobile"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_46"
                app:layout_constraintTop_toBottomOf="@+id/viewLine1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >

                <ImageView
                    android:id="@+id/ivMobile"
                    android:layout_width="@dimen/dp_20"
                    android:layout_height="@dimen/dp_20"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginStart="@dimen/dp_20"
                    android:src="@drawable/ic_icon_mobile"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:textColor="#000000"
                    android:text="手机号"
                    app:layout_constraintTop_toTopOf="@+id/ivMobile"
                    app:layout_constraintStart_toEndOf="@+id/ivMobile"
                    app:layout_constraintBottom_toBottomOf="@+id/ivMobile"
                    android:layout_marginStart="@dimen/dp_10"
                    />

                <ImageView
                    android:layout_width="@dimen/dp_16"
                    android:layout_height="@dimen/dp_16"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginEnd="@dimen/dp_19"
                    android:src="@drawable/ic_icon_next"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/viewLine2"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                app:layout_constraintTop_toBottomOf="@+id/clMobile"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:background="#d6d6d6"
                />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clQQ"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_46"
                app:layout_constraintTop_toBottomOf="@+id/viewLine2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >

                <ImageView
                    android:id="@+id/ivQQ"
                    android:layout_width="@dimen/dp_20"
                    android:layout_height="@dimen/dp_20"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginStart="@dimen/dp_20"
                    android:src="@drawable/ic_icon_qq"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:textColor="#000000"
                    android:text="手机号"
                    app:layout_constraintTop_toTopOf="@+id/ivQQ"
                    app:layout_constraintStart_toEndOf="@+id/ivQQ"
                    app:layout_constraintBottom_toBottomOf="@+id/ivQQ"
                    android:layout_marginStart="@dimen/dp_10"
                    />

                <ImageView
                    android:layout_width="@dimen/dp_16"
                    android:layout_height="@dimen/dp_16"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginEnd="@dimen/dp_19"
                    android:src="@drawable/ic_icon_next"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/viewLine3"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                app:layout_constraintTop_toBottomOf="@+id/clQQ"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:background="#d6d6d6"
                />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clWeChat"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_46"
                app:layout_constraintTop_toBottomOf="@+id/viewLine3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                >

                <ImageView
                    android:id="@+id/ivWeChat"
                    android:layout_width="@dimen/dp_20"
                    android:layout_height="@dimen/dp_20"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginStart="@dimen/dp_20"
                    android:src="@drawable/ic_icon_wechat"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:textColor="#000000"
                    android:text="微信"
                    app:layout_constraintTop_toTopOf="@+id/ivWeChat"
                    app:layout_constraintStart_toEndOf="@+id/ivWeChat"
                    app:layout_constraintBottom_toBottomOf="@+id/ivWeChat"
                    android:layout_marginStart="@dimen/dp_10"
                    />

                <ImageView
                    android:layout_width="@dimen/dp_16"
                    android:layout_height="@dimen/dp_16"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginEnd="@dimen/dp_19"
                    android:src="@drawable/ic_icon_next"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <Button
            android:id="@+id/btnBack"
            android:layout_width="@dimen/dp_335"
            android:layout_height="@dimen/dp_40"
            android:layout_gravity="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/clSettingLayout"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_20"
            android:background="@drawable/bg_yellow_25_corner"
            android:text="退出"
            android:textSize="@dimen/sp_14"
            android:textColor="#fff"
            />

        <TextView
            android:id="@+id/tvLastLogin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/btnBack"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginBottom="@dimen/dp_109"
            android:textSize="@dimen/sp_12"
            android:text="最後登入時間：2020-02-10 11:22:35"
            android:gravity="center"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>